    <EmbeddedResource Include="sass\_button.scss" />
    <EmbeddedResource Include="sass\_card.scss" />
    <EmbeddedResource Include="sass\_color.scss" />
    <EmbeddedResource Include="sass\_container.scss" />
    <EmbeddedResource Include="sass\_display.scss" />
    <EmbeddedResource Include="sass\_functions.scss" />
    <EmbeddedResource Include="sass\_grid.scss" />
    <EmbeddedResource Include="sass\_hr.scss" />
    <EmbeddedResource Include="sass\_image.scss" />
    <EmbeddedResource Include="sass\_preview.scss" />
    <EmbeddedResource Include="sass\_reboot_email.scss" />
    <EmbeddedResource Include="sass\_spacing.scss" />
    <EmbeddedResource Include="sass\_table.scss" />
    <EmbeddedResource Include="sass\_typography.scss" />
    <EmbeddedResource Include="sass\_variables.scss" />
    <EmbeddedResource Include="template.html.erb" />
    <EmbeddedResource Include="templates\align-center.html.erb" />
    <EmbeddedResource Include="templates\align-left.html.erb" />
    <EmbeddedResource Include="templates\align-right.html.erb" />
    <EmbeddedResource Include="templates\body.html.erb" />
