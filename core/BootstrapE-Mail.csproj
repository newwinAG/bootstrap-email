<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>BootstrapE_Mail</RootNamespace>
    <AssemblyVersion>0.1.1.0</AssemblyVersion>
    <FileVersion>0.1.1.0</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>0.1.1</Version>
    <PackageId>BootstrapEMailCSharp</PackageId>
    <Authors>Newwin SWS</Authors>
    <Company>Newwin SWS</Company>
    <Product>Bootstrap E-Mail C#</Product>
    <Description>This Library converts a Bootstrap 4 Page, so that it should look the same on most to all E-Mail HTML Clients</Description>
    <PackageProjectUrl>https://github.com/newwinAG/bootstrap-email</PackageProjectUrl>
    <Copyright>Copyright New-win SWS AG</Copyright>
    <RepositoryUrl>https://github.com/newwinAG/bootstrap-email</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Bootstrap,E-Mail</PackageTags>
    <PackageReleaseNotes>Corrected Inline CSS File handling</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="bootstrap-email.scss" />
    <None Remove="head.scss" />
    <None Remove="sass\_alert.scss" />
    <None Remove="sass\_badge.scss" />
    <None Remove="sass\_border.scss" />
    <None Remove="sass\_button.scss" />
    <None Remove="sass\_card.scss" />
    <None Remove="sass\_color.scss" />
    <None Remove="sass\_container.scss" />
    <None Remove="sass\_display.scss" />
    <None Remove="sass\_functions.scss" />
    <None Remove="sass\_grid.scss" />
    <None Remove="sass\_hr.scss" />
    <None Remove="sass\_image.scss" />
    <None Remove="sass\_preview.scss" />
    <None Remove="sass\_reboot_email.scss" />
    <None Remove="sass\_spacing.scss" />
    <None Remove="sass\_table.scss" />
    <None Remove="sass\_typography.scss" />
    <None Remove="sass\_variables.scss" />
    <None Remove="template.html.erb" />
    <None Remove="templates\align-center.html.erb" />
    <None Remove="templates\align-left.html.erb" />
    <None Remove="templates\align-right.html.erb" />
    <None Remove="templates\body.html.erb" />
    <None Remove="templates\col.html.erb" />
    <None Remove="templates\container.html.erb" />
    <None Remove="templates\div.html.erb" />
    <None Remove="templates\hr.html.erb" />
    <None Remove="templates\row.html.erb" />
    <None Remove="templates\table-left.html.erb" />
    <None Remove="templates\table.html.erb" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="bootstrap-email.scss" />
    <EmbeddedResource Include="head.scss" />
    <EmbeddedResource Include="sass\_alert.scss" />
    <EmbeddedResource Include="sass\_badge.scss" />
